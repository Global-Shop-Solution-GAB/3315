program.sub.cmdserialadd_click.start
F.Intrinsic.Control.SetErrorHandler("cmdserialadd_click_Err")
F.Intrinsic.Control.ClearErrors

V.Local.sError.Declare(String)

V.Local.i.Declare(Long)
V.Local.iMsg.Declare(Long)
V.Local.sSQL.Declare(String)
V.Local.sMsg.Declare(String)
V.Local.sAlreadyExist.Declare(String)
V.Local.sTemp.Declare(String)
V.Local.iCreateNew.Declare(Long)


F.Intrinsic.Control.If(V.Screen.SS_Form1!txtSerialStart.Text,=,"")
	F.Intrinsic.UI.Msgbox("Please enter a serial number.")
	F.Intrinsic.Control.ExitSub
F.Intrinsic.Control.EndIf

F.Intrinsic.Control.If(V.Screen.SS_Form1!txtSerialEnd.Text,=,"")
	F.Intrinsic.UI.Msgbox("Please enter a serial number.")
	F.Intrinsic.Control.ExitSub
F.Intrinsic.Control.EndIf

'============find and store serials that already exist and the serial numbers that do not exist from the serial range entered on the GAB screen.===========
V.Global.sCreateNew.Redim(0,0)
V.Local.sAlreadyExist.Set("")
F.Intrinsic.Control.For(V.Local.i,V.Screen.SS_Form1!txtSerialStart.Text,V.Screen.SS_Form1!txtSerialEnd.Text,1)
	F.Intrinsic.String.Concat("Select * From GCG_3315__WARRANTY_GEN Where SERIAL = '",V.local.i,"'",V.Local.sSQL)
	F.ODBC.Connection!con.OpenRecordsetRO("rst",V.Local.sSQL)
	F.Intrinsic.Control.If(V.ODBC.con!rst.EOF,=,False)
		F.Intrinsic.String.Concat(V.Local.sAlreadyExist,V.ODBC.con!rst.FieldValTrim!SERIAL,",",V.Local.sAlreadyExist)
	F.Intrinsic.Control.Else
		F.Intrinsic.String.Concat(V.Global.sCreateNew,V.Local.i,",",V.Global.sCreateNew)
	F.Intrinsic.Control.EndIf
	F.ODBC.con!rst.Close
F.Intrinsic.Control.Next(V.Local.i)
'===================================End this portion===================================================================================


'============Checks arrays for serial numbers that exist or do not exist.=====================================================================
F.Intrinsic.Control.If(V.Local.sAlreadyExist,=,"")
	'if blank then no serial numbers exist within range, add the whole serial number range to flexgrid.
	F.Intrinsic.Control.CallSub(Createnewserial)
F.Intrinsic.Control.Else
	'If not blank then some or all serial numbers exist with serial range, check if any new serial numbers should be created
	F.Intrinsic.Control.If(V.Global.sCreateNew,=,"")
		F.Intrinsic.UI.MsgBox("All serial numbers already exist within the serial range.","GAB Message")
		F.Intrinsic.Control.ExitSub
	F.Intrinsic.Control.Else
		'If not blank then new serial numbers can be added to flexgrid, ask if user wants to create new serial numbers from range that have not been assigned.
		'Removes the comma at the end of each array
		F.Intrinsic.String.Len(V.Local.sAlreadyExist,V.Local.iCreateNew)
		F.Intrinsic.Math.Sub(V.Local.iCreateNew,1,V.Local.iCreateNew)
		F.Intrinsic.String.Left(V.Local.sAlreadyExist,V.Local.iCreateNew,V.Local.sAlreadyExist)
