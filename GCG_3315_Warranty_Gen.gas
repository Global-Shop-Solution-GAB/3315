V.Local.sRowArrayTwo.Declare(String)

F.Intrinsic.Control.If(V.Screen.SS_Form1!txtWarrantyID.Text,=,"")
	F.Intrinsic.UI.Msgbox("Please select a Warranty Plan ID.")
	F.Intrinsic.Control.ExitSub
F.Intrinsic.Control.EndIf

F.Intrinsic.UI.Msgbox("Are you sure you want to issue warranties?","GAB Msg",4,V.Local.iMsg)
F.Intrinsic.Control.If(V.Local.iMsg,=,7)
	F.Intrinsic.Control.ExitSub
F.Intrinsic.Control.Else
	F.Intrinsic.Math.Sub(V.Screen.SS_Form1!gsflexgrid1.Rows,1,V.Local.iRow)
	F.Intrinsic.Math.Sub(V.Screen.Form3!gsflexUser.Rows,1,V.Local.iRowTwo)
	F.Intrinsic.Control.For(V.Local.i,1,V.Local.iRow,1)
		Gui.SS_Form1.gsflexgrid1.ReadRow(V.Local.i,V.Local.sRow)
		F.Intrinsic.String.Split(V.Local.sRow,"*!*",V.Local.sRowArray)
		F.Intrinsic.Control.If(V.Local.sRowArray(1),=,"N")
			F.Intrinsic.String.Concat("Select * From GCG_3315__WARRANTY_GEN where SERIAL = '",V.Local.sRowArray(0).Trim,"'",V.Local.sSQL)
			F.ODBC.Connection!con.OpenRecordsetRW("rst",V.Local.sSQL)
			F.Intrinsic.Control.If(V.ODBC.con!rst.EOF,=,True)
				F.ODBC.con!rst.AddNew
				F.ODBC.con!rst.Set!SERIAL(V.Local.sRowArray(0))
				F.Intrinsic.String.LPad(V.Screen.SS_Form1!txtOrderNo.Text,0,7,V.Local.sTemp)
				F.ODBC.con!rst.Set!ORDER_NO(V.Local.sTemp)
				F.Intrinsic.String.LPad(V.Screen.SS_Form1!txtOrderLn.Text,0,3,V.Local.sTemp)
				F.Intrinsic.String.RPad(V.Local.sTemp,0,4,V.Local.sTemp)
				F.ODBC.con!rst.Set!ORDER_LINE(V.Local.sTemp)
				F.ODBC.con!rst.Set!PART(V.Screen.SS_Form1!txtPartNo.Text)
				F.ODBC.con!rst.Set!WARRANTY_PLAN_ID(V.Screen.SS_Form1!txtWarrantyID.Text)
				F.ODBC.con!rst.Set!LASTNAME(V.Screen.SS_Form1!txtNameL.Text)
				F.ODBC.con!rst.Set!FIRSTNAME(V.Screen.SS_Form1!txtNameF.Text)
				F.ODBC.con!rst.Set!ADDRESS1(V.Screen.SS_Form1!txtAddress1.Text)
				F.ODBC.con!rst.Set!ADDRESS2(V.Screen.SS_Form1!txtAddress2.Text)
				F.ODBC.con!rst.Set!CITY(V.Screen.SS_Form1!txtCity.Text)
				F.ODBC.con!rst.Set!STATE(V.Screen.SS_Form1!txtState.Text)
				F.ODBC.con!rst.Set!ZIP(V.Screen.SS_Form1!txtZip.Text)
				F.ODBC.con!rst.Set!PHONE(V.Screen.SS_Form1!txtPhoneH.Text)
				F.ODBC.con!rst.Set!WORK_PHONE(V.Screen.SS_Form1!txtPhoneW.Text)
				V.Local.dDate.Set(V.Screen.SS_Form1!dtpPurchase.Value)
				F.ODBC.con!rst.Set!PURCHASE_DATE(V.Local.dDate.PervasiveDate)
				F.ODBC.con!rst.Set!PREFIX(V.Screen.SS_Form1!txtPrefix.Text)
				F.ODBC.con!rst.Update
				Gui.SS_Form1.gsflexgrid1.TextMatrix(1,V.Local.i,"Y")
				Gui.SS_Form1.gsflexgrid1.ApplyStyle(V.Local.i,1)
			F.Intrinsic.Control.EndIf
			F.ODBC.con!rst.Close
			'User Fields 1- 25
			F.Intrinsic.Control.For(V.Local.iA,1,V.Local.iRowTwo,1)
				Gui.Form3.gsflexUser.ReadRow(V.Local.iA,V.Local.sRowTwo)
				F.Intrinsic.String.Split(V.Local.sRowTwo,"*!*",V.Local.sRowArrayTwo)
				F.Intrinsic.Control.If(V.Local.sRowArrayTwo(0),<=,25)
					F.Intrinsic.String.Concat("Select * From GCG_3315__WARRANTY_USER25 Where SERIAL = '",V.Local.sRowArray(0).Trim,"'",V.Local.sSQL)
					F.ODBC.Connection!con.OpenRecordsetRW("rst",V.Local.sSQL)
					F.Intrinsic.Control.If(V.ODBC.con!rst.EOF,=,True)
						F.ODBC.con!rst.AddNew
						F.ODBC.con!rst.Set!SERIAL(V.Local.sRowArray(0))
						F.ODBC.con!rst.Set!USER[V.Local.sRowArrayTwo(0)](V.Local.sRowArrayTwo(2))
					F.Intrinsic.Control.Else
						F.ODBC.con!rst.Set!USER[V.Local.sRowArrayTwo(0)](V.Local.sRowArrayTwo(2))
					F.Intrinsic.Control.EndIf
					F.ODBC.con!rst.Update
					F.ODBC.con!rst.Close
				F.Intrinsic.Control.EndIf
				'User Fields 26- 50
				F.Intrinsic.Control.If(V.Local.sRowArrayTwo(0),>,25)
					F.Intrinsic.Control.If(V.Local.sRowArrayTwo(0),<=,50)
						F.Intrinsic.String.Concat("Select * From GCG_3315__WARRANTY_USER50 Where SERIAL = '",V.Local.sRowArray(0).Trim,"'",V.Local.sSQL)
						F.ODBC.Connection!con.OpenRecordsetRW("rst",V.Local.sSQL)
						F.Intrinsic.Control.If(V.ODBC.con!rst.EOF,=,True)
							F.ODBC.con!rst.AddNew
							F.ODBC.con!rst.Set!SERIAL(V.Local.sRowArray(0))
							F.ODBC.con!rst.Set!USER[V.Local.sRowArrayTwo(0)](V.Local.sRowArrayTwo(2))
						F.Intrinsic.Control.Else
							F.ODBC.con!rst.Set!USER[V.Local.sRowArrayTwo(0)](V.Local.sRowArrayTwo(2))
						F.Intrinsic.Control.EndIf
						F.ODBC.con!rst.Update
						F.ODBC.con!rst.Close
					F.Intrinsic.Control.EndIf
				F.Intrinsic.Control.EndIf
				'User Fields 51- 75
				F.Intrinsic.Control.If(V.Local.sRowArrayTwo(0),>,51)
					F.Intrinsic.String.Concat("Select * From GCG_3315__WARRANTY_USER75 Where SERIAL = '",V.Local.sRowArray(0).Trim,"'",V.Local.sSQL)
					F.ODBC.Connection!con.OpenRecordsetRW("rst",V.Local.sSQL)
					F.Intrinsic.Control.If(V.ODBC.con!rst.EOF,=,True)
						F.ODBC.con!rst.AddNew
						F.ODBC.con!rst.Set!SERIAL(V.Local.sRowArray(0))
						F.ODBC.con!rst.Set!USER[V.Local.sRowArrayTwo(0)](V.Local.sRowArrayTwo(2))
					F.Intrinsic.Control.Else
						F.ODBC.con!rst.Set!USER[V.Local.sRowArrayTwo(0)](V.Local.sRowArrayTwo(2))
					F.Intrinsic.Control.EndIf
					F.ODBC.con!rst.Update
					F.ODBC.con!rst.Close
				F.Intrinsic.Control.EndIf
			F.Intrinsic.Control.Next(V.Local.iA)
		F.Intrinsic.Control.EndIf
	F.Intrinsic.Control.Next(V.Local.i)
F.Intrinsic.Control.EndIf

'F.Intrinsic.Control.CallSub(Clearall)
F.Intrinsic.UI.Msgbox("Save successful!")
F.Intrinsic.Control.CallSub(Unload)

	F.Intrinsic.Control.ExitSub

F.Intrinsic.Control.Label("cmdsave_click_Err")
F.Intrinsic.Control.If(V.Ambient.ErrorNumber,<>,0)
	Function.Intrinsic.String.Concat("Project: GCG_Warranty_Gen.gas",V.Ambient.Newline,V.Ambient.Newline,"Subroutine: ",V.Ambient.CurrentSubroutine,V.Ambient.NewLine,"Error Occurred ",V.Ambient.ErrorNumber," with description ",V.Ambient.ErrorDescription,V.Local.sError)
	F.Intrinsic.UI.Msgbox(V.Local.sError)
	F.Intrinsic.Control.CallSub(Unload)
Function.Intrinsic.Control.EndIf


