Program.Sub.CreateNewSerial.Start
F.Intrinsic.Control.SetErrorHandler("CreateNewSerial_Err")
F.Intrinsic.Control.ClearErrors

V.Local.sError.Declare(String)

V.Local.i.Declare(Long)
V.Local.iR.Declare(Long,1)
V.Local.iRow.Declare(Long)
V.Local.iCreateNew.Declare(Long)
V.Local.iErrorCalc.Declare(Long)
V.Local.iSerialEnd.Declare(Long)
V.Local.iSerialStart.Declare(Long)
V.Local.sMsg.Declare(String)

V.Local.iSerialEnd.Set(V.Screen.SS_Form1!txtSerialEnd.Text)
V.Local.iSerialStart.Set(V.Screen.SS_Form1!txtSerialStart.Text)
F.Intrinsic.Control.CallSub(Qtycheck)
F.Intrinsic.Control.If(V.Global.iCount,>=,V.Global.iQtyOrdered)
	F.Intrinsic.UI.Msgbox("NO new serial numbers can be added for this Sales Order Line.  Records were created prior to this warranty for this Sales Order Line. The amount of serial numbers already created are equal to the order qty.","GAB Validation")
	F.Intrinsic.Control.ExitSub
F.Intrinsic.Control.Else
	F.Intrinsic.Math.Sub(V.Local.iSerialEnd,V.Local.iSerialStart,V.Local.iErrorCalc)
	F.Intrinsic.Math.Add(V.Local.iErrorCalc,1,V.Local.iErrorCalc)
	F.Intrinsic.Control.If(V.Global.iCount,=,0)
		F.Intrinsic.Control.If(V.Global.iQtyOrdered,<>,V.Local.iErrorCalc)
			F.Intrinsic.UI.Msgbox("The number of serial numbers being issued is greater or less than the order qty. Please update the number of serial numbers being issued or the order qty. No serial numbers have been issued.","GAB Validation")
			F.Intrinsic.Control.ExitSub
		F.Intrinsic.Control.EndIf
	F.Intrinsic.Control.Else
		'at this point, icount has to be less than qty ordered but greater than 0
		F.Intrinsic.Math.Sub(V.Global.iQtyOrdered,V.Global.iCount,V.Global.sQtyRemaining)
		F.Intrinsic.Control.If(V.Global.sQtyRemaining.Long,<>,V.Local.iErrorCalc)
			F.Intrinsic.String.Concat(V.Global.sQtyRemaining," serial number(s) can be added to this Sales Order Line.  Records were created prior to this warranty for this Sales Order Line. The amount of serial numbers created prior to do not exceed the order qty.",V.Local.sMsg)
			F.Intrinsic.UI.Msgbox(V.Local.sMsg,"GAB Validation")
			F.Intrinsic.Control.ExitSub
		F.Intrinsic.Control.EndIf
	F.Intrinsic.Control.EndIf
F.Intrinsic.Control.EndIf
'Remove Comma at end of Array
F.Intrinsic.String.Len(V.Global.sCreateNew,V.Local.iCreateNew)
F.Intrinsic.Math.Sub(V.Local.iCreateNew,1,V.Local.iCreateNew)
F.Intrinsic.String.Left(V.Global.sCreateNew,V.Local.iCreateNew,V.Global.sCreateNew)
'========================================

'Add serial numbers not assigned to flexgrid
F.Intrinsic.String.Split(V.Global.sCreateNew,",",V.Global.sCreateNew)
Gui.SS_Form1.gsflexgrid1.Visible(False)
F.Intrinsic.Control.For(V.Local.i,V.Global.sCreateNew.LBound,V.Global.sCreateNew.UBound,1)
	F.Intrinsic.Math.Add(V.Local.iR,1,V.Local.iR)
	F.Intrinsic.Math.Sub(V.Local.iR,1,V.Local.iRow)
	Gui.SS_Form1.gsflexgrid1.Rows(V.Local.iR)
	Gui.SS_Form1.gsflexgrid1.TextMatrix(0,V.Local.iRow,V.Global.sCreateNew(v.Local.i))
	Gui.SS_Form1.gsflexgrid1.TextMatrix(1,V.Local.iRow,"N")
	Gui.SS_Form1.gsflexgrid1.ApplyStyle(V.Local.iRow,1)
F.Intrinsic.Control.Next(V.Local.i)
Gui.SS_Form1.gsflexgrid1.Visible(True)
'==================================================

Gui.SS_Form1.cmdSave.Enabled(True)

F.Intrinsic.Control.ExitSub

F.Intrinsic.Control.Label("CreateNewSerial_Err")
F.Intrinsic.Control.If(V.Ambient.ErrorNumber,<>,0)
	Function.Intrinsic.String.Concat("Project: GCG_Warranty_Gen.gas",V.Ambient.Newline,V.Ambient.Newline,"Subroutine: ",V.Ambient.CurrentSubroutine,V.Ambient.NewLine,"Error Occurred ",V.Ambient.ErrorNumber," with description ",V.Ambient.ErrorDescription,V.Local.sError)
